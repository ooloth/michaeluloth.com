{
  "name": "mu-astro",
  "type": "module",
  "version": "0.0.1",
  "dependencies": {
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/tailwind": "^4.0.0",
    "astro": "^2.8.4",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.11.0",
    "prettier-plugin-toml": "^0.3.1"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "npm i && astro dev",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "lint": "eslint '**/*.{astro,js,ts,tsx}' --ignore-path .gitignore",
    "prettier": "prettier \"**/*.+(astro|cjs|css|graphql|html|js|json|jsonc|jsx|md|mdx|mjs|toml|ts|tsx|xml|yaml|yml)\"",
    "preview": "astro preview",
    "start": "npm run dev",
    "types:check": "astro check && tsc --noEmit",
    "validate": "npm run format:check && npm run lint && npm run types:check"
  }
}
